        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var start = new GlideDateTime(current.user_story.test_period.start_date);
    var end = new GlideDateTime(current.user_story.test_period.end_date);
    var created = new GlideDateTime(current.sys_created_on);
	if(created.getDate() == end.getDate() || created.getDate() == start.getDate()){
		current.out_of_test_period = false;
	} else if (created.after(end)) {
        current.out_of_test_period = true;
    } else {
        current.out_of_test_period = false;
    }

})(current, previous);]]></script>
