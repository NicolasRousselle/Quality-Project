        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var start = new GlideDateTime(current.start_date);
    var end = new GlideDateTime(current.end_date);
	if (end.before(start)) {
        gs.addErrorMessage("End date cannot be before Start date.  Your period has not been saved to the database.");
        current.setAbortAction(true);
    } else if (start.after(end)) {
        gs.addErrorMessage("Start date cannot be after End date.  Your period has not been saved to the database.");
        current.setAbortAction(true);
    }

})(current, previous);]]></script>
